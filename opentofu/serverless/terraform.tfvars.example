# Copy this file to terraform.tfvars and fill in your values.

region      = "us-east-2"
stack_name  = "compound-docs-serverless"
environment = "dev"

# Network
vpc_cidr = "10.1.0.0/16"

# Authentication (comma-separated API keys)
# api_keys = "key1,key2"

# Lambda
lambda_memory_size = 512
lambda_timeout     = 30

# GitSync
# gitsync_image_digest = "sha256:..."  # Set by CI
gitsync_schedule = "rate(6 hours)"
