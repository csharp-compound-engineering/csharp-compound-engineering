{
  "$schema": "https://claude.ai/schemas/marketplace-plugin/v1.json",
  "name": "CSharp Compound Docs",
  "version": "0.1.0-preview",
  "description": "Intelligent documentation management with semantic search and compound document orchestration for Claude.",
  "longDescription": "CSharp Compound Docs transforms how you interact with documentation using the compound engineering paradigm. It enables semantic understanding, intelligent retrieval, and contextual knowledge synthesis powered by AI embeddings and vector search.",
  "author": {
    "name": "CSharp Compound Engineering Team",
    "email": "team@example.com",
    "url": "https://github.com/your-org/csharp-compound-engineering"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/csharp-compound-engineering.git"
  },
  "homepage": "https://github.com/your-org/csharp-compound-engineering",
  "bugs": {
    "url": "https://github.com/your-org/csharp-compound-engineering/issues"
  },
  "license": "MIT",
  "keywords": [
    "documentation",
    "knowledge-management",
    "rag",
    "semantic-search",
    "embeddings",
    "compound-documents",
    "mcp",
    "csharp",
    "dotnet"
  ],
  "categories": [
    "documentation",
    "knowledge-management",
    "rag"
  ],
  "icon": "./icon.svg",
  "screenshots": [
    {
      "path": "./screenshots/semantic-search.png",
      "caption": "Semantic search powered by AI embeddings"
    },
    {
      "path": "./screenshots/orchestration.png",
      "caption": "Compound document orchestration"
    },
    {
      "path": "./screenshots/knowledge-graph.png",
      "caption": "Knowledge graph visualization"
    },
    {
      "path": "./screenshots/rag-responses.png",
      "caption": "RAG-powered contextual responses"
    }
  ],
  "engines": {
    "claude": ">=1.0.0"
  },
  "runtime": {
    "type": "dotnet",
    "version": "9.0"
  },
  "mcp": {
    "serverType": "stdio",
    "command": "dotnet",
    "args": [
      "run",
      "--project",
      "${installPath}/src/CompoundDocs.McpServer/CompoundDocs.McpServer.csproj"
    ],
    "env": {
      "DOTNET_ENVIRONMENT": "Production"
    },
    "capabilities": {
      "tools": true,
      "resources": true,
      "prompts": true
    },
    "tools": [
      {
        "name": "rag_query",
        "description": "RAG-powered knowledge retrieval from compound documents"
      },
      {
        "name": "semantic_search",
        "description": "Vector similarity search across indexed documents"
      },
      {
        "name": "index_document",
        "description": "On-demand document indexing with embedding generation"
      },
      {
        "name": "list_doc_types",
        "description": "Enumerate available document type schemas"
      },
      {
        "name": "search_external_docs",
        "description": "Search external documentation sources"
      },
      {
        "name": "rag_query_external",
        "description": "RAG queries over external documentation"
      },
      {
        "name": "delete_documents",
        "description": "Remove documents from the index"
      },
      {
        "name": "update_promotion_level",
        "description": "Manage document promotion levels"
      },
      {
        "name": "activate_project",
        "description": "Switch active project context"
      }
    ],
    "skills": [
      { "name": "/cdocs:capture-problem", "description": "Capture problem documentation" },
      { "name": "/cdocs:capture-insight", "description": "Capture insight documentation" },
      { "name": "/cdocs:capture-codebase", "description": "Capture codebase documentation" },
      { "name": "/cdocs:capture-tool", "description": "Capture tool documentation" },
      { "name": "/cdocs:capture-style", "description": "Capture style documentation" },
      { "name": "/cdocs:query", "description": "Query the knowledge base" },
      { "name": "/cdocs:search", "description": "Semantic search across documents" },
      { "name": "/cdocs:recall", "description": "Recall related documents" },
      { "name": "/cdocs:related", "description": "Find related documents" },
      { "name": "/cdocs:activate", "description": "Activate a project" },
      { "name": "/cdocs:deactivate", "description": "Deactivate current project" },
      { "name": "/cdocs:status", "description": "Show system status" },
      { "name": "/cdocs:promote", "description": "Promote document level" },
      { "name": "/cdocs:demote", "description": "Demote document level" },
      { "name": "/cdocs:delete", "description": "Delete documents" },
      { "name": "/cdocs:reindex", "description": "Reindex documents" },
      { "name": "/cdocs:help", "description": "Show help information" }
    ],
    "resources": [
      {
        "name": "documents",
        "description": "Access indexed documentation resources"
      },
      {
        "name": "knowledge-graph",
        "description": "Access the documentation knowledge graph"
      }
    ]
  },
  "permissions": {
    "required": [
      {
        "type": "filesystem",
        "access": "read",
        "description": "Read documentation files for indexing"
      },
      {
        "type": "network",
        "access": "localhost",
        "description": "Connect to local PostgreSQL database"
      }
    ],
    "optional": [
      {
        "type": "network",
        "access": "outbound",
        "description": "Connect to external embedding providers (OpenAI, Azure)"
      },
      {
        "type": "filesystem",
        "access": "write",
        "description": "Write cached embeddings and temporary files"
      }
    ]
  },
  "configuration": {
    "schema": {
      "type": "object",
      "properties": {
        "connectionString": {
          "type": "string",
          "description": "PostgreSQL connection string with pgvector",
          "format": "uri"
        },
        "embeddingModel": {
          "type": "string",
          "description": "Semantic Kernel embedding model identifier",
          "default": "text-embedding-ada-002"
        },
        "chunkSize": {
          "type": "integer",
          "description": "Document chunk size for indexing",
          "default": 1000,
          "minimum": 100,
          "maximum": 8000
        },
        "chunkOverlap": {
          "type": "integer",
          "description": "Overlap between document chunks",
          "default": 200,
          "minimum": 0,
          "maximum": 1000
        },
        "maxResults": {
          "type": "integer",
          "description": "Maximum number of search results",
          "default": 10,
          "minimum": 1,
          "maximum": 100
        },
        "similarityThreshold": {
          "type": "number",
          "description": "Minimum similarity score for results",
          "default": 0.7,
          "minimum": 0.0,
          "maximum": 1.0
        },
        "logLevel": {
          "type": "string",
          "description": "Logging verbosity level",
          "enum": ["Trace", "Debug", "Information", "Warning", "Error", "Critical"],
          "default": "Information"
        }
      },
      "required": ["connectionString"]
    }
  },
  "dependencies": {
    "external": [
      {
        "name": "PostgreSQL",
        "version": ">=16.0",
        "required": true,
        "description": "Database with pgvector extension"
      },
      {
        "name": "pgvector",
        "version": ">=0.5.0",
        "required": true,
        "description": "Vector similarity search extension for PostgreSQL"
      }
    ],
    "nuget": [
      {
        "name": "Microsoft.SemanticKernel",
        "version": "1.70.0"
      },
      {
        "name": "ModelContextProtocol",
        "version": ">=0.1.0"
      },
      {
        "name": "Npgsql.EntityFrameworkCore.PostgreSQL",
        "version": ">=9.0.0"
      },
      {
        "name": "Pgvector",
        "version": ">=0.3.0"
      }
    ]
  },
  "components": {
    "skills": [
      { "name": "/cdocs:capture-problem", "description": "Capture problem documentation" },
      { "name": "/cdocs:capture-insight", "description": "Capture insight documentation" },
      { "name": "/cdocs:capture-codebase", "description": "Capture codebase documentation" },
      { "name": "/cdocs:capture-tool", "description": "Capture tool documentation" },
      { "name": "/cdocs:capture-style", "description": "Capture style documentation" },
      { "name": "/cdocs:query", "description": "Query the knowledge base" },
      { "name": "/cdocs:search", "description": "Semantic search across documents" },
      { "name": "/cdocs:recall", "description": "Recall related documents" },
      { "name": "/cdocs:related", "description": "Find related documents" },
      { "name": "/cdocs:activate", "description": "Activate a project" },
      { "name": "/cdocs:deactivate", "description": "Deactivate current project" },
      { "name": "/cdocs:status", "description": "Show system status" },
      { "name": "/cdocs:promote", "description": "Promote document level" },
      { "name": "/cdocs:demote", "description": "Demote document level" },
      { "name": "/cdocs:delete", "description": "Delete documents" },
      { "name": "/cdocs:reindex", "description": "Reindex documents" },
      { "name": "/cdocs:help", "description": "Show help information" }
    ]
  },
  "preview": true,
  "publishedAt": null,
  "updatedAt": null
}
